<h1>Listing personas</h1>


<table>
  <tr>
    <th>Tipo</th>
    <th>Nombre</th>
    <th>Apepat</th>
    <th>Apemat</th>
    <th>Sexo</th>
    <th>Tipdoc</th>
    <th>Numdoc</th>
    <th>Correo</th>
    <th>Password</th>
    <th>Fecnac</th>
    <th>Telefono</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @personas.each do |persona| %>
  <tr>
    <td><%= persona.tipo %></td>
    <td><%= persona.nombre %></td>
    <td><%= persona.apepat %></td>
    <td><%= persona.apemat %></td>
    <td><%= persona.sexo %></td>
    <td><%= persona.tipdoc %></td>
    <td><%= persona.numdoc %></td>
    <td><%= persona.correo %></td>
    <td><%= persona.password %></td>
    <td><%= persona.fecnac %></td>
    <td><%= persona.telefono %></td>
    <td><%= link_to 'Show', persona %></td>
    <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
    <td><%= link_to 'Destroy', persona, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

